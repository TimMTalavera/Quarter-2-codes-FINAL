students = int(input("Enter number of students: "))
subjects = int(input("Enter number of subjects: "))

total_class = 0
count_class = 0



for s in range(1, students + 1):
    print("Student", s)
    total = 0

    for x in range(1, subjects + 1):
        score = float(input("Enter score " + str(x) + ": "))
        total += score

    avg = total / subjects
    print("Average for Student", s, "=", avg)
    print()

    total_class += total
    count_class += subjects

class_avg = total_class / count_class
print("Class Average =", class_avg)
